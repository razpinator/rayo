{
  "name": "Rayo LSP Client",
  "contributes": {
    "languages": [{
      "id": "rayo",
      "aliases": ["Rayo"],
      "extensions": [".ryo"],
      "configuration": "./language-configuration.json"
    }],
    "configuration": {
      "rayo.lsp.server": {
        "type": "string",
        "default": "localhost:2087",
        "description": "Rayo LSP server address"
      }
    }
  },
  "activationEvents": ["onLanguage:rayo"],
  "main": "./client.js"
}
