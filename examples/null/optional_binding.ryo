# Optional Binding
# Demonstrates pattern matching with optionals

def get_user(id) {
    users = {"1": {"name": "Alice"}}
    return users.get(id)
}

def main() {
    user = get_user("1")
    if user != None {
        print("User found: " + user["name"])
    } else {
        print("User not found")
    }

    # Safe navigation
    name = user?.name or "Unknown"
    print("Name: " + name)
}
